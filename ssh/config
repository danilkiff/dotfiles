Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes

Host *.cnbs.lan
    User pikachu
    IdentityFile ~/.ssh/id_ed25519
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    SetEnv LC_ALL="en_US.UTF-8"
    SetEnv LC_CTYPE="en_US.UTF-8"

Host *
    VisualHostKey yes